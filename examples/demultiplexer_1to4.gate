CIRCUIT Demultiplexer1to4 {
  INPUT Input, S0, S1;
  OUTPUT Y0, Y1, Y2, Y3;
  WIRE notS0, notS1, and_temp0, and_temp1, and_temp2, and_temp3;
  notS0 = NOT(S0);
  notS1 = NOT(S1);
  and_temp0 = AND(notS0, notS1);
  Y0 = AND(Input, and_temp0);
  and_temp1 = AND(notS0, S1);
  Y1 = AND(Input, and_temp1);
  and_temp2 = AND(S0, notS1);
  Y2 = AND(Input, and_temp2);
  and_temp3 = AND(S0, S1);
  Y3 = AND(Input, and_temp3);
}

